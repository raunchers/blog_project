<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="newTitle">Title</label>
    <input class="form-control" type="text" name="newTitle" autocomplete="off">
    <label for="newEntry">Post</label>
    <textarea class="form-control" name="newEntry" rows="3" autocomplete="off"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
